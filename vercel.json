{
  "build": {
    "functions": {
      "api/**/*.ts": {
        "runtime": "nodejs14.x"
      }
    },
    "command": "npx prisma generate"
  },
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ]
}
